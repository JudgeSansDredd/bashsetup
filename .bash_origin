source ~/.bash/.bash_aliases
source ~/.bash/.bash_variables
source ~/.bash/.bash_history
source ~/.bash/.bash_path
source ~/.bash/.zsh_numpad

fpath=( ~/.bash/zshfunctions "${fpath[@]}" )
# autoload _vcomm
autoload ~/.bash/zshfunctions/*

if [ -f ~/.bash/.bash_custom ]; then
	source ~/.bash/.bash_custom
fi

